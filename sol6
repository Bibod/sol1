using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace sol6
{
    internal class Program
    {
        static void Main(string[] args)
        {
            float a, b, c;
            Console.WriteLine("Intruduceti a =");
            a = float.Parse(Console.ReadLine());
            Console.WriteLine("Introduceti b =");
            b = float.Parse(Console.ReadLine());
            Console.WriteLine("Introduceti numarul c =");
            c = float.Parse(Console.ReadLine());
            if (a >=1 && b >=1 && c >=1) 
                if (a + b > c && a + c > b && c + b > a  )
                {
                    Console.WriteLine(" Numerele a, b, c pot fi laturi al unui triunghi");
                }
            else
                {
                    Console.WriteLine("Numerele a, b, c nu pot fi laturi al unui triunghi");
                }
        }
    }
}
